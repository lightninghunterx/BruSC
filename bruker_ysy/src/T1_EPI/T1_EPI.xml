                            <parameter name="InvSliceSpoiler"/>
                        </column>
                    </parameterCard>
                </parameter>
            </column>
            <column>
                <textLine text=""/>
            </column>
        </parameterCard>
        <xi:include href="file:///XML_INCLUDE_DIR/Freq1.xml"/>
        <xi:include href="file:///XML_INCLUDE_DIR/Tx1.xml"/>
    </parameterCard>
