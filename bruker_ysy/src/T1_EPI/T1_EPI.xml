                <parameter name="ExcPulseAmpl.patt" displayName="Exc. Pulse Attenuation" widget="slider"/>
                <textLine text=""/>
                <parameter name="RefPulseAmpl.patt" displayName="Rfc. Pulse Attenuation" widget="slider"/>
