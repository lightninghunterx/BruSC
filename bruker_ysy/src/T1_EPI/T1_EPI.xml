    <xi:include href="file:///XML_INCLUDE_DIR/EpiInc.xml"/>
    /parameterCard -->
    <parameterCard displayName="Sequence">
        <parameterCard displayName="Main">
            <column>
                <textLine text=""/>
                <parameter name="PVM_EffSWh"/>
