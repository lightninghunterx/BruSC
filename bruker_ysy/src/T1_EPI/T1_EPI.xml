                <parameter name="InvPulseAmpl.patt" displayName="Inv. Pulse Attenuation" widget="slider"/>
		<textLine text=""/>
		<parameter name="PVM_RgJob" />
		<parameter name="PVM_RgValue" widget="slider" />
