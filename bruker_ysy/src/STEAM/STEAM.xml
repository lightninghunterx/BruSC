                <textLine text=""/>
                <parameter name="VoxPul2Ampl.patt" displayName="Pulse 2 Attenuation" widget="slider"/>
                <textLine text=""/>
                <parameter name="VoxPul3Ampl.patt" displayName="Pulse 3 Attenuation" widget="slider"/>
                <textLine text=""/>
		<parameter name="PVM_RgJob" />
		<parameter name="PVM_RgValue" widget="slider" />
            </column>
            <column>
                <textLine text=""/>
                <parameter name="PVM_RefPowStat1"/>
                <textLine text=""/>
                <parameter name="LockVoxPulAmpls"/>
                <parameter name="VoxPul1Ampl.ppow"  displayName="Pulse 1 Power"/>
                <parameter name="VoxPul1Ampl.pampl" displayName="Pulse 1 Amplitude"/>
                <parameter name="VoxPul2Ampl.ppow"  displayName="Pulse 2 Power"/>
                <parameter name="VoxPul2Ampl.pampl" displayName="Pulse 2 Amplitude"/>
                <parameter name="VoxPul3Ampl.ppow"  displayName="Pulse 3 Power"/>
                <parameter name="VoxPul3Ampl.pampl" displayName="Pulse 3 Amplitude"/>
            </column>
        </parameterCard>
        <xi:include href="file:///XML_INCLUDE_DIR/SuppressionSetup.xml"/>
        <xi:include href="file:///XML_INCLUDE_DIR/OvsSetup.xml"/>
        <xi:include href="file:///XML_INCLUDE_DIR/mapshim.xml"/>
        <xi:include href="file:///XML_INCLUDE_DIR/Shim.xml"/>
    </parameterCard>
    <parameterCard displayName="Result" cardType="adjresult">
        <column>
            <textLine text="Receiver Gain Result:"/>
            <textLine text="Local Frequency Result:"/>
