    </parameterCard>
    <parameterCard displayName="Spectroscopy">
        <xi:include href="file:///XML_INCLUDE_DIR/Spec1D.xml"/>
    </parameterCard>
    <parameterCard displayName="Preparation">
        <parameterCard displayName="Main">
            <column>
                <parameter name="PVM_DummyScans"/>
