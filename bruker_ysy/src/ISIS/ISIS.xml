                <parameter name="PVM_DecOnOff"/>
                <parameter name="PVM_NoeOnOff"/>
                <parameter name="PVM_TriggerModule"/>
            </column>
        </parameterCard>
        <xi:include href="file:///XML_INCLUDE_DIR/Suppression.xml"/>
        <parameterCard displayName="OVS">
            <column>
                <parameter name="PVM_OvsOnOff"/>
                <parameter name="IsisNumOvsCycles"/>
                <parameter name="PVM_OvsSliceThickness" displayName="Slice Thickness"/>
                <parameter name="PVM_OvsGapToVoxel" displayName="Gap to Voxel"/>
                <parameter name="PVM_OvsChemShiftDisplacement"/>
                <parameter name="PVM_OvsConflict"/>
            </column>
            <column>
                <textLine text=""/>
                <parameter name="PVM_OvsPulse1Enum">
                    <parameterCard displayName="Pulse Details">
                        <column>
                            <parameter name="PVM_OvsPulse1"/>
                        </column>
                    </parameterCard>
                </parameter>
                <parameter name="PVM_OvsSpoilerDuration"/>
                <parameter name="PVM_OvsSpoilerStrength" style="inline_array"/>
                <parameter name="PVM_OvsModuleDuration"/>
            </column>
        </parameterCard>
        <xi:include href="file:///XML_INCLUDE_DIR/SatTrans.xml"/>
        <xi:include href="file:///XML_INCLUDE_DIR/Decoupling.xml"/>
        <xi:include href="file:///XML_INCLUDE_DIR/Noe.xml"/>
        <xi:include href="file:///XML_INCLUDE_DIR/Trigg.xml"/>
    </parameterCard>
    <parameterCard displayName="Optimization">
        <column>
            <parameter name="IsisManAdjustment" displayName="Manual Pulse Adjustment"/>
            <parameter name="IsisFov"/>
            <parameter name="PVM_Matrix[0]"/>
            <parameter name="PVM_EffSWh" displayName="Acq. Bandwidth"/>
        </column>
        <column>
            <textLine text=""/>
        </column>
    </parameterCard>
    <parameterCard displayName="Sequence">
        <parameterCard displayName="Main">
            <column>
                <textLine text=""/>
                <parameter name="ExcPul1Enum" displayName="Excitation Pulse">
                    <parameterCard displayName="Pulse Details">
                        <column>
                            <parameter name="ExcPul1"/>
                        </column>
                    </parameterCard>
                </parameter>
                <parameter name="InvPul1Enum" displayName="Inversion Pulse">
                    <parameterCard displayName="Pulse Details">
                        <column>
                            <parameter name="InvPul1"/>
