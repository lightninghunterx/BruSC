            <column>
                <parameter name="Method"/>
                <textLine text=""/>
                <textLine text=""/>
                <textLine text=""/>
                <textLine text=""/>
                <parameter name="IsisSpoilCyMm1"/>
                <parameter name="IsisSpoilCyMm2"/>
                <parameter name="IsisSpoilCyMm3"/>
