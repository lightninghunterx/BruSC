        <xi:include href="file:///XML_INCLUDE_DIR/FovSatSetup.xml"/>
        <xi:include href="file:///XML_INCLUDE_DIR/SatTransSetup.xml"/>
        <xi:include href="file:///XML_INCLUDE_DIR/mapshim.xml"/>
        <xi:include href="file:///XML_INCLUDE_DIR/Shim.xml"/>
