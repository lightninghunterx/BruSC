<cardStack
    xmlns="http://www.bruker.de/mri/generated/xsd/card"
    xmlns:xi="http://www.w3.org/2001/XInclude">
    <parameterCard displayName="Routine">
        <column>
            <parameter name="PVM_EchoTime"/>
            <parameter name="PVM_RepetitionTime"/>
            <parameter name="PVM_NAverages"/>
            <parameter name="PVM_NRepetitions"/>
            <parameter name="PVM_ScanTimeStr"/>
            <textLine text=""/>
            <parameter name="SPExamExcEnum" displayName="Examined Exc. Pulse">
                <parameterCard displayName="Pulse Details">
                    <column>
                        <parameter name="SPExamExc"/>
                    </column>
                </parameterCard>
            </parameter>
            <parameter name="SPExamRfcEnum" displayName="Examined Rfc. Pulse">
                <parameterCard displayName="Pulse Details">
                    <column>
                        <parameter name="SPExamRfc"/>
                    </column>
                </parameterCard>
            </parameter>
            <parameter name="SPExamInvEnum" displayName="Examined Inv. Pulse">
                <parameterCard displayName="Pulse Details">
                    <column>
                        <parameter name="SPExamInv"/>
                    </column>
                </parameterCard>
            </parameter>
            <parameter name="HardPulse.Length" displayName="Duration of Hardpulse">
                <parameterCard displayName="Hard Pulse Details">
                    <column>
                        <parameter name="HardPulse"/>
                    </column>
                </parameterCard>
            </parameter>
        </column>
        <column>
            <parameter name="GradientDirection"/>
            <parameter name="ProfileMode"/>
            <parameter name="InversionTime"/>
            <parameter name="PVM_Matrix"/>
            <parameter name="PVM_Fov"/>
            <textLine text=""/>
            <parameter name="DeriveExaminedPulseGain"/>
            <parameter name="DeriveHardPulseGain"/>
            <parameter name="PulseAlg" displayName="If calculated,
						    then:"/>
	    <parameter name="PVM_AtsBedPositionMode"/>
        </column>
    </parameterCard>
    <parameterCard displayName="Sequence">
        <parameterCard displayName="Main">
            <column>
                <textLine text=""/>
                <parameter name="PVM_EffSWh"/>
                <parameter name="Spoiler_duration"/>
                <parameter name="Ndummy"/>
            </column>
            <column>
                <parameter name="Method"/>
            </column>
        </parameterCard>
        <xi:include href="file:///XML_INCLUDE_DIR/Freq1.xml"/>
        <xi:include href="file:///XML_INCLUDE_DIR/Tx1.xml"/>
    </parameterCard>
    <parameterCard displayName="Setup">
        <parameterCard displayName="Main">
            <column>
                <parameter name="DeriveExaminedPulseGain"/>
                <parameter name="SPExamExcAmpl.patt" displayName="Exc. Pulse Attenuation" widget="slider"/>
                <parameter name="SPExamExcAmpl.ppow" displayName="Exc. Pulse Power"/>
                <parameter name="SPExamExcAmpl.pampl" displayName="Exc. Pulse Amplitude"/>
                <parameter name="SPExamRfcAmpl.patt" displayName="Rfc. Pulse Attenuation" widget="slider"/>
                <parameter name="SPExamRfcAmpl.ppow" displayName="Rfc. Pulse Power"/>
                <parameter name="SPExamRfcAmpl.pampl" displayName="Rfc. Pulse Amplitude"/>
                <parameter name="SPExamInvAmpl.patt" displayName="Inv. Pulse Attenuation" widget="slider"/>
                <parameter name="SPExamInvAmpl.ppow" displayName="Inv. Pulse Power"/>
                <parameter name="SPExamInvAmpl.pampl" displayName="Inv. Pulse Amplitude"/>
            <textLine text=""/>
	    <parameter name="PVM_RgJob" />
	    <parameter name="PVM_RgValue" widget="slider" />
            <textLine text=""/>
            </column>
            <column>
                <textLine text=""/>
                <parameter name="DeriveHardPulseGain"/>
                <parameter name="HardPulseAmpl.patt" displayName="Hard Pulse Attenuation" widget="slider"/>
                <parameter name="HardPulseAmpl.ppow" displayName="Hard Pulse Power"/>
                <parameter name="HardPulseAmpl.pampl" displayName="Hard Pulse Amplitude"/>
            </column>
        </parameterCard>
        <xi:include href="file:///XML_INCLUDE_DIR/Shim.xml"/>
    </parameterCard>
    <parameterCard displayName="System">
        <xi:include href="file:///XML_INCLUDE_DIR/CoilSelection.xml"/>
    </parameterCard>
</cardStack>
